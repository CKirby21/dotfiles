Host 192.168.0.27 172.22.*.* 10.22.*.*
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	LogLevel ERROR
	User root
	PubkeyAcceptedAlgorithms +ssh-rsa
	HostkeyAlgorithms +ssh-rsa
	# IdentityFile ~/.ssh/id_rsa-dev@redtail
	# IdentitiesOnly yes

Host 0.0.0.0 localhost
	Port 2222
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	LogLevel ERROR
	User root
	PubkeyAcceptedAlgorithms +ssh-rsa
	HostkeyAlgorithms +ssh-rsa
	IdentityFile ~/.ssh/id_rsa-dev@redtail
	IdentitiesOnly yes

